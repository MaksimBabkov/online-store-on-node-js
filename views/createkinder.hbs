        <span>File</span>
        <input type="file" name="filedate" multiple>
      </div>
      <div class="file-path-wrapper">
        <input class="file-path validate" type="text" placeholder="Upload one or more files">
      </div>
    </div>


<input class="btn" type="submit" value="Отправить" />

        </form>
